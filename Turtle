import turtle
a=5
print(a)
b='hello'
print(b)

#def square
def square():
  p_turtle=turtle.Turtle()
  p_turtle.forward(100)
  p_turtle.right(90)
  p_turtle.forward(100)
  p_turtle.right(90)
  p_turtle.forward(100)
  p_turtle.right(90)
  p_turtle.forward(100)
  p_turtle.color("light green")
square()
turtle.forward(300)
square()
